include "assert";
include "funcs";

(
  ([
    ".",
    ".a",
    ".a[0]",
    ".a[123].bb",
    ".[123].a",
    ".[123][123].a",
    ".\"b b\"",
    ".\"a \\\\ b\"",
    ".\"a \\\" b\""
  ][] | assert("\(.) | expr_to_path | path_to_expr"; .; expr_to_path | path_to_expr))
,
  ([
    [[], []],
    [[1], [[1]]],
    [[1,1], [[1,1]]],
    [[1,1,2], [[1,1],[2]]],
    [[1,1,2,2], [[1,1],[2,2]]],
    [[1,2,2,1], [[1],[2,2],[1]]]
  ][] | assert("\(.) | streaks"; .[1]; .[0] | streaks))
,
  ([
    [[{a:1},{a:1},{a:2}], [[{a:1},{a:1}],[{a:2}]]]
  ][] | assert("\(.) | streaks_by"; .[1]; .[0] | streaks_by(.a)))
)
