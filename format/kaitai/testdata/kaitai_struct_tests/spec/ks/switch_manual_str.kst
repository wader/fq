id: switch_manual_str
data: switch_opcodes.bin
asserts:
  - actual: opcodes.size
    expected: 4

  - actual: opcodes[0].code
    expected: '"S"'
  - actual: opcodes[0].body.as<switch_manual_str::opcode::strval>.value
    expected: '"foobar"'

  - actual: opcodes[1].code
    expected: '"I"'
  - actual: opcodes[1].body.as<switch_manual_str::opcode::intval>.value
    expected: 0x42

  - actual: opcodes[2].code
    expected: '"I"'
  - actual: opcodes[2].body.as<switch_manual_str::opcode::intval>.value
    expected: 0x37

  - actual: opcodes[3].code
    expected: '"S"'
  - actual: opcodes[3].body.as<switch_manual_str::opcode::strval>.value
    expected: '""'
